{
  "connect": {
    "name": "camera._connect",
    "parameters": {
      "hw_time": "{hw_time}",
      "time_zone": "GMT+8"
    }
  },
  "start_preview": {
    "name": "camera._startPreview",
    "parameters": {
      "origin": {
        "mime": "h264",
        "width": 1920,
        "height": 1440,
        "framerate": 30,
        "bitrate": 20480
      },
      "stiching": {
        "mode": "pano",
        "mime": "h264",
        "width": 3840,
        "height": 1920,
        "framerate": 30,
        "bitrate": 10240
      },
      "audio": {
        "mime":"aac", 
        "sampleFormat":"s16",
        "channelLayout":"stereo",
        "samplerate":48000,
        "bitrate":128
      }
    },
    "stabilization": true
  },
  "take_picture": {
    "name": "camera._takePicture",
    "parameters": {
      "origin": {
        "mime": "jpeg",
        "width": 4000,
        "height": 3000,
        "saveOrigin": true,
        "storage_loc": 1
      },
      "stiching": {
        "mode": "pano",
        "mime": "jpeg",
        "width": 7680,
        "height": 3840,
        "map": "equirectangular",
        "algorithm": "normal"
      }
    }
  },
  "stop_preview": {
    "name": "camera._stopPreview"
  },
  "get_result": {
    "name": "camera._getResult",
    "parameters": {
      "list_ids": ["{sequence}"]
    }
  },
  "start_live": {
    "name": "camera._startLive",
    "parameters": {
      "origin": {
        "mime": "h264",
        "width": 3840,
        "height": 2880,
        "framerate": 24,
        "bitrate": 10000,
        "logMode": 0,
        "liveUrl": "rtmp://192.168.1.188:1935/live",
        "saveOrigin": false
      }
    },
    "stabilization": false
  },
  "stop_live": {
    "name": "camera._stopLive"
  }
}